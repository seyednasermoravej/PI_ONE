{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "LaunchF",
            "cwd": "${workspaceFolder}",
            "executable": "build/zephyr/zephyr.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "interface": "swd",
            "configFiles": [ "interface\\stlink.cfg", "target\\stm32f4x.cfg"],
            "gdbPath": "${userHome}/zephyr-sdk-0.16.5/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
            "preLaunchTask": "buildFlash",
            "showDevDebugOutput": "raw",
            "rtos": "Zephyr",
            "overrideLaunchCommands": [
                "interpreter-exec console \"monitor reset halt\"",
                //'target-download',
                //'interpreter-exec console \"monitor reset halt\"',
                "-enable-pretty-printing"
            ],
            // "breakAfterReset": true

        },
        {
            "name": "LaunchG",
            "cwd": "${workspaceFolder}",
            "executable": "build/zephyr/zephyr.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "gdbPath": "${userHome}/zephyr-sdk-0.16.5/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
            "interface": "swd",
            "configFiles": [ "interface\\stlink.cfg", "target\\stm32fgx.cfg"],
            "preLaunchTask": "West Build",
            "showDevDebugOutput": "raw",
            "rtos": "Zephyr",
            // "breakAfterReset": true
        },
        {
            "name": "Attach",
            "device": "STM32F407VG",
            // "device": "nRF52840_xxAA",
            "cwd": "${workspaceFolder}",
            "executable": "build/zephyr/zephyr.elf",
            "request": "attach",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            // "servertype": "stlink",

            "servertype": "openocd",
            // "servertype": "jlink",
            "configFiles": [ "/board/stm32f4discovery.cfg"],
            "gdbPath": "${userHome}/zephyr-sdk-0.16.5/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb"
        },
    ]
}